<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>

    <display-name>JetriX Admin</display-name>
    <description>Web administration console for JetriX</description>

    <servlet>
        <servlet-name>server</servlet-name>
        <servlet-class>org.apache.jsp.server_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>channel</servlet-name>
        <servlet-class>org.apache.jsp.channel_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user</servlet-name>
        <servlet-class>org.apache.jsp.user_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>settings</servlet-name>
        <servlet-class>org.apache.jsp.settings_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>navigation</servlet-name>
        <servlet-class>org.apache.jsp.navigation_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>navigation-channels</servlet-name>
        <servlet-class>org.apache.jsp.navigationChannels_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>navigation-users</servlet-name>
        <servlet-class>org.apache.jsp.navigationUsers_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/server.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>channel</servlet-name>
        <url-pattern>/channel.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/user.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>settings</servlet-name>
        <url-pattern>/settings.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>navigation</servlet-name>
        <url-pattern>/navigation.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>navigation-channels</servlet-name>
        <url-pattern>/navigationChannels.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>navigation-users</servlet-name>
        <url-pattern>/navigationUsers.jsp</url-pattern>
    </servlet-mapping>

</web-app>
